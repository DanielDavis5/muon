a = {'a': 'b', 'c': 'd', 'e': {'f': {'g': {'h': [1, 2, {'3': 'secret!'}]}}}}

assert(a['a'] == 'b')
assert(a['c'] == 'd')
# TODO: index chaining
# assert(a['e']['f']['g']['h'][2]['3'] == 'secret!')

count = 0
foreach x, y : a
    count += 1
endforeach

assert(count == 3)

b = {'a': 1, 'b': 2}
b += {'c': 3}
assert(b['a'] == 1)
assert(b['c'] == 3)
b += {'a': 2}
assert(b['a'] == 2)

assert(b['c'].to_string() == '3')

dict = {'a': 0, 'b': 0, 'c': 0}
assert(dict.keys().length() == 3)
foreach k : dict.keys()
    dict += {k: 7}
endforeach

assert(dict['a'] == 7)
assert(dict['b'] == 7)
assert(dict['c'] == 7)

